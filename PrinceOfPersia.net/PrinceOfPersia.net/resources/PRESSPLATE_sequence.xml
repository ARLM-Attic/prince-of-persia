<?xml version="1.0" encoding="utf-8"?>
<ArrayOfSequence xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">

  <!-- PressPlate -->
  <Sequence>
    <frames>
      <Frame>
        <type>SPRITE</type>
        <name>LOOP</name>
        <value>Tiles/FloorC</value>
        <stoppable>true</stoppable>
        <xOffSet>0</xOffSet>
        <yOffSet>0</yOffSet>
      </Frame>
      <Frame>
        <type>COMMAND</type>
        <name>GOTOFRAME</name>
        <parameter>LOOP</parameter>
        <stoppable>true</stoppable>
      </Frame>
    </frames>
    <name>NORMAL</name>
    <raised>false</raised>
  </Sequence>

  <!-- PRESS DOWN -->
  <Sequence>
    <frames>
      <Frame>
        <type>SPRITE</type>
        <name>LOOP</name>
        <value>Tiles/FloorA</value>
        <stoppable>true</stoppable>
        <xOffSet>0</xOffSet>
        <yOffSet>0</yOffSet>
      </Frame>
      <Frame>
      <type>COMMAND</type>
      <name>GOTOFRAME</name>
      <parameter>LOOP</parameter>
      <stoppable>true</stoppable>
      </Frame>
    </frames>
    <name>DPRESSPLATE</name>
    <raised>false</raised>
  </Sequence>

</ArrayOfSequence>

